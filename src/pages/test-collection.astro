---
import { getCollection } from "astro:content";

console.log('=== TEST COLLECTION PAGE ===');

try {
  const posts = await getCollection("blog");
  console.log('getCollection("blog") success');
  console.log('Total posts found:', posts.length);
  console.log('Post slugs:', posts.map(p => p.slug));
  
  const testSlug = "01-astro-sphere-file-structure";
  const found = posts.find(post => post.slug === testSlug);
  console.log(`Found "${testSlug}":`, found ? 'YES' : 'NO');
  
} catch (error) {
  console.error('getCollection("blog") failed:', error);
}

console.log('===========================');
